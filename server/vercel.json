{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/places", "dest": "/index.js" },
    { "src": "/user-places", "dest": "/index.js" },
    { "src": "/user-places", "methods": ["PUT"], "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
